{
  "name": "atlas-collector",
  "version": "1.0.0",
  "description": "User interaction tracking library",
  "main": "dist/php/logCollector.min.js",
  "scripts": {
    "build": "pnpm lint && npx esbuild src/php/index.ts --bundle --outfile=dist/php/logCollector.iife.js --format=iife --global-name=LogCollector --target=es2017",
    "build:min": "pnpm lint && npx esbuild src/php/index.ts --bundle --outfile=dist/php/logCollector.min.js --format=iife --global-name=LogCollector --target=es2017 --minify",
    "watch": "npx esbuild src/php/index.ts --bundle --outfile=dist/php/logCollector.iife.js --format=iife --global-name=LogCollector --target=es2017 --watch",
    "lint": "eslint ."
  },
  "keywords": [
    "analytics",
    "tracking",
    "logging"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "esbuild": ">=0.25.0",
    "eslint": "^9.27.0",
    "eslint-plugin-security": "^3.0.1",
    "typescript": "^5.0.0"
  }
}